<!-- %Tag(FULL)%-->
<launch>

    <!-- joy node -->
    <node respawn="true" pkg="joy" type="joy_node" name="tri_joy" >
        <param name="dev" type="string" value="/dev/input/js1" />
        <param name="deadzone" value="0" />
    </node>

    <node pkg="tricopter" type="groundstation.py" name="tri_tx"  output="screen" />
    <node pkg="tricopter" type="serialmon.py"     name="tri_rx"  output="screen" />
    <node pkg="tricopter" type="visualization.py" name="tri_vis" output="screen" />

</launch>
<!-- %EndTag(FULL)%-->
